{
  "expo": {
    "name": "VastuGuru",
    "slug": "VastuGuru",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/vastu.png",
    "scheme": "vastuguru",
    "entryPoint": "expo-router/entry",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/vastu.png",
        "backgroundImage": "./assets/images/vastu.png",
        "monochromeImage": "./assets/images/vastu.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.satish0804.VastuGuru"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/vastu.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/vastu.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6bcfc725-5c92-412b-a308-3d47e645a9d9"
      }
    }
  }
}
